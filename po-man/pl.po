#: ../man/free.1:58
#, no-wrap
msgid "B<-b>, B<--bytes>"
msgstr "B<-b>, B<--bytes>"

#. type: Plain text
#: ../man/free.1:61
msgid "Display the amount of memory in bytes."
msgstr "Wyświetlanie ilości pamięci w bajtach."

#. type: TP
#: ../man/free.1:61
#, no-wrap
msgid "B<-k>, B<--kibi>"
msgstr "B<-k>, B<--kibi>"

#. type: Plain text
#: ../man/free.1:64
msgid "Display the amount of memory in kibibytes.  This is the default."
msgstr "Wyświetlanie ilości pamięci w kibibajtach (zachowanie domyślne)."

#. type: TP
#: ../man/free.1:64
#, no-wrap
msgid "B<-m>, B<--mebi>"
msgstr "B<-m>, B<--mebi>"

#. type: Plain text
#: ../man/free.1:67
