#. type: TP
#: ../man/free.1:67
#, no-wrap
msgid "B<-g>, B<--gibi>"
msgstr "B<-g>, B<--gibi>"

#. type: Plain text
#: ../man/free.1:70
msgid "Display the amount of memory in gibibytes."
msgstr "Afișează cantitatea de memorie în gibiocteți."

#. type: TP
#: ../man/free.1:70
#, no-wrap
msgid "B<--tebi>"
msgstr "B<--tebi>"

#. type: Plain text
#: ../man/free.1:73
msgid "Display the amount of memory in tebibytes."
