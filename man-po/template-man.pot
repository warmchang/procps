msgstr ""

#. type: TP
#: ./pgrep.1:83
#, no-wrap
msgid "B<-a>, B<--list-full>"
msgstr ""

#. type: Plain text
#: ./pgrep.1:88
msgid "List the full command line as well as the process ID.  (B<pgrep> only.)"
msgstr ""

#. type: TP
#: ./pgrep.1:88
#, no-wrap
msgid "B<-n>, B<--newest>"
msgstr ""

