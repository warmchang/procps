#. type: Plain text
#: ../man/procps_misc.3:51
#, no-wrap
msgid "Mask Name Resolving\n"
msgstr "Masknamnsuppslagning\n"

#. type: Plain text
#: ../man/procps_misc.3:55
#, no-wrap
msgid ""
