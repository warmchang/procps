#. type: TP
#: ./free.1:20
#, no-wrap
msgid "B<used>"
msgstr ""

#. type: Plain text
#: ./free.1:23
msgid "Used memory (calculated as total - free)"
msgstr ""

#. type: TP
#: ./free.1:23
#, no-wrap
msgid "B<free>"
msgstr ""

